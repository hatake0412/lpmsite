
<table>
  <tr>
    <th>Name</th>
    <th>Ver</th>
    <th>Description</th>
    <th>Depends</th>
    <th>Cnt</th>
    <th>FAQ</th>
  </tr>

<% @packages.each do |package| %>
  <tr>
    <td><a href="<%= package.note %>"><%= package.name %></a></td>
    <td><%= package.ver %></td>
    <td><%= package.description %></td>
    <td><%= package.depends %></td>
    <td><%= link_to '動いたよ (' << package.cnt.to_s << ')' %></td>
    <td><%= link_to 'FAQ', faq_response_path(package.id) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Package', new_package_path %>
